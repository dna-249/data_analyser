{
  "version": 2,
  "builds": [
    {
      "src": "myapp/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "myapp/wsgi.py"
    }
  ],
  "functions": {
    "myapp/wsgi.py": {
      "excludeFiles": "staticfiles/**"
    }
  }
}